# rsi code example


POSACT(CPX, CPY, CPZ, CPA, CPB, CPC)
AXISACT(AP1, AP2, AP3, AP4, AP5, AP6)

COMMUNICATION(1)

PRINT(KX)
PRINT(KY)
PRINT(KZ)
PRINT(KA)
PRINT(KB)
PRINT(KC)
PRINT(FR)
PRINT(STOP)

IF EQ(STOP, 1) THEN
	STOP(1)
ELSE 
	POSCORR0(KX, KY, KZ, KA, KB, KC)
	# AXISCORR0(0, 0, KX, 0, 90, 0)
END_IF

